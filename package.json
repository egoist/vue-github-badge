{
  "name": "vue-github-badge",
  "version": "0.0.0",
  "description": "cuz I need it",
  "repository": {
    "url": "egoist/vue-github-badge",
    "type": "git"
  },
  "main": "dist/vue-github-badge.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo 'no tests!' && npm run lint && npm run build",
    "lint": "xo",
    "build": "bili ./src/index.js --format umd --compress --module-name GitHubBadge --name vue-github-badge --js babel",
    "build:example": "vbuild",
    "deploy": "npm run build:example && gh-pages -d dist-example",
    "dev": "vbuild -d"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "bili": "^0.14.0",
    "eslint-config-rem": "^3.0.0",
    "gh-pages": "^0.12.0",
    "rollup-plugin-babel": "^2.7.1",
    "vbuild": "^6.23.2",
    "xo": "^0.17.1"
  },
  "xo": {
    "extends": "rem",
    "esnext": true
  },
  "vbuild": {
    "dist": "dist-example",
    "entry": "example.js"
  }
}
